{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "Node16",
    "moduleResolution": "Node16",
    "noEmit": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2016"
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}
